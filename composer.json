{
    "name": "revenuewire/ach-file",
    "type": "library",
    "description": "Create and read ACH Payment files",
    "homepage": "https://github.com/revenuewire/ach-payment-file",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Matthew Casiro",
            "email": "mattcasiro@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RW\\": "src/"
        }
    },
    "autoload-dev": {
